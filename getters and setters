package ecssem2;
class Human{
	private int age;
	private String name;
	public Human()
	{
		age=12;
		name="john";
	}
	public Human(int a,String n) 
	{
	age=a;
	name=n;
	}
	

	public int getage()
	{
		return age;
	}
	public void setage(int a)
	{
		this.age=a;
	}
	public String getname()
	{
		return name ;
	}
	public void setname(String n)
	{
		this.name = n;
	}
		
	}



public class Demo {

	public static void main(String[] args) {
	Human obj=new Human();
	Human obj1=new Human(18,"shashwat"); 
	//obj.setage();
	// obj.setname(); 
	System.out.println(obj.getname()+" : "+obj.getage());
	System.out.println(obj1.getname()+" : "+obj.getage());
	}

}
